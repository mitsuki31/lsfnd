{
  "name": "lsfnd",
  "version": "0.1.0",
  "title": "LSFND",
  "description": "A small Node.js library to ease files and directories listing featuring filter support.",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist/**/*",
    "types/index.d.ts",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./lsfnd": "./dist/lsfnd.js",
    "./types": "./types/index.d.ts",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "tsc",
    "build": "ts-node scripts/build.ts",
    "minify": "ts-node scripts/build.ts --minify",
    "docs": "npm run-script build:docs",
    "build:docs": "typedoc --options typedoc.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitsuki31/lsfnd.git"
  },
  "author": "Ryuu Mitsuki <dhefam31@gmail.com> (https://github.com/mitsuki31)",
  "contributors": [
    "Ryuu Mitsuki <dhefam31@gmail.com> (https://github.com/mitsuki31)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mitsuki31/lsfnd/issues"
  },
  "homepage": "https://github.com/mitsuki31/lsfnd#readme",
  "devDependencies": {
    "@types/node": "^20.12.4",
    "esbuild": "^0.20.2",
    "ts-node": "~10.9.2",
    "typedoc-material-theme": "^1.0.2",
    "typedoc-plugin-extras": "^3.0.0",
    "typescript": "~5.4.3"
  },
  "peerDependencies": {
    "typedoc": "^0.25.12"
  },
  "keywords": [
    "ls",
    "list",
    "javascript",
    "typescript",
    "node",
    "nodejs",
    "library"
  ]
}
